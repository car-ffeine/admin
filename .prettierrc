{
  "endOfLine": "auto",
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 100,
  "semi": true,
  "trailingComma": "es5",
  "importOrder": [
    "^react(.*)",
    "^@util/(.*)$",
    "^@store/(.*)$",
    "^@mock",
    "^@mock/(.*)$",
    "^@hook/(.*)$",
    "^@common/(.*)$",
    "^@component/(.*)$",
    "^@style",
    "^@constant",
    "^@constant/(.*)$",
    "^@type",
    "^App",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
